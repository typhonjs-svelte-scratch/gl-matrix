{
  "version": "4.0.0",
  "name": "gl-matrix",
  "description": "Javascript Matrix and Vector library for High Performance 3D web apps",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "homepage": "http://glmatrix.net",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/gl-matrix.js",
      "require": "./dist/cjs/gl-matrix.js"
    },
    "./src": {
      "types": "./types/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./src/modern": {
      "types": "./types/index-modern.d.ts",
      "import": "./dist/src/index-modern.js"
    },
    "./package.json": "./package.json"
  },
  "bugs": {
    "url": "https://github.com/toji/gl-matrix/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/toji/gl-matrix.git"
  },
  "contributors": [
    {
      "name": "Brandon Jones",
      "email": "tojiro@gmail.com"
    },
    {
      "name": "Colin MacKenzie IV",
      "email": "sinisterchipmunk@gmail.com"
    }
  ],
  "scripts": {
    "build": "tsc",
    "build-docs": "typedoc src/index.ts",
    "bundle": "node scripts/bundle.mjs",
    "bundle-types": "rollup -c",
    "watch": "tsc --watch",
    "test": "vitest run"
  },
  "files": [
    "/dist",
    "/types"
  ],
  "devDependencies": {
    "esbuild": "^0.15.15",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3",
    "rollup": "^3.21.1",
    "rollup-plugin-dts": "^5.3.0",
    "tslib": "^2.5.0",
    "vitest": "^0.25.2"
  }
}
